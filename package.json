    "@trpc/client": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "@trpc/tanstack-react-query": "^11.8.1",
    "better-auth": "^1.4.10",
    "better-sqlite3": "^12.5.0",
    "drizzle-orm": "^0.45.1",
    "isbot": "^5",
    "qrcode": "^1.5.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.11.0",
    "zod": "^4.3.5"
