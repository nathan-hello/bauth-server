{
  "name": "natav",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "vite",
    "start": "react-router start",
    "typegen:rr": "react-router typegen && tsc",
    "typegen:ba": "cli generate --config ./server/auth.ts --output ./server/drizzle/schema.auth.ts -y",
    "typegen:db": "drizzle-kit generate --config ./server/drizzle/config.ts",
    "typegen": "typegen:rr && bun run typegen:ba && bun run typegen:db",
    "db:migrate": "drizzle-kit migrate --config ./server/drizzle/config.ts",
    "db:push": "drizzle-kit push --config ./server/drizzle/config.ts",
    "db:studio": "drizzle-kit studio --config ./server/drizzle/config.ts"
  },
  "dependencies": {
    "@better-auth/passkey": "^1.4.10",
