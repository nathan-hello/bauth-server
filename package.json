{
  "name": "natav",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bunx react-router build",
    "dev": "bunx --bun vite ",
    "start": "bun --env-file=.env ./build/server/index.js",
    "typegen:rr": "bunx react-router typegen && tsc",
    "typegen:ba": "bunx --bun cli generate --config ./server/auth.ts --output ./server/drizzle/schema.auth.ts -y",
    "typegen:db": "bunx drizzle-kit generate --config ./server/drizzle/config.ts",
    "typegen": "bun run typegen:rr && bun run typegen:ba && bun run typegen:db",
    "db:migrate": "bunx drizzle-kit migrate --config ./server/drizzle/config.ts",
    "db:push": "bunx drizzle-kit push --config ./server/drizzle/config.ts",
    "db:studio": "bunx drizzle-kit studio --config ./server/drizzle/config.ts"
  },
  "dependencies": {
    "@libsql/client": "^0.15.10",
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "@spotify/web-api-ts-sdk": "^1.2.0",
    "@tanstack/react-query": "^5.83.0",
    "@trpc/client": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "@trpc/tanstack-react-query": "^11.4.3",
    "better-auth": "^1.3.3",
    "drizzle-orm": "^0.44.3",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.1",
    "react-router-hono-server": "^2.16.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.18",
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.4",
    "@types/bun": "^1.2.19",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "drizzle-kit": "^0.31.4",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "prettier": {
    "tabwidth": 8
  }
}
