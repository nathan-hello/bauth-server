    "@types/react-dom": "^19.2.3",
    "drizzle-kit": "^0.31.8",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
